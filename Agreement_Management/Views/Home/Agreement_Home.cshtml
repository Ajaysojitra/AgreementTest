@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="row">
        <div class="col-sm-4"></div>
       
            <div class="form-group">
                <h3>Welcome to Agreement Home Page</h3>
                <input type="button" id="btnpopu" value="New Agreement" onclick="ShowPopup();" />  
                <script type="text/javascript">  
                    ShowPopup = function ()
                    {  
                         window.open('/Home/New_Agreement', "PopupWindow", 'width=400px,height=600px,top=150,left=250');  
                    }  
                </script>
            <input type="button" id="loguot" value="Logout" onclick="Redirect();" />
                <script type="text/javascript">
                    Redirect = function ()
                    {
                    HttpContext.Session.Clear();
                    return RedirectToAction("Index", "Home");
                   
                    }
                 </script>
             </div>
        <link href="~/css/jquery.datatables.css" rel="stylesheet" />
        <script src="~/js/jquery.datatables.min.js"></script>
        <div class="container">
            <div class="table-responsive">
                <table id="myTable" class="table table-bordered">
                    <thead>
                        <tr>
                            <th>User Name</th>
                            <th>Product Group Code</th>
                            <th>Product Number</th>
                            <th>Effective Date</th>
                            <th>Expiration Date </th>
                            <th>Product Price</th>
                            <th>New Price</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                </table>
                <script>
                    $(document).ready(function () {
                        $('#myTable').DataTable({
                            processing: true,
                            serverSide: true
                        });
                    });

                </script>
            </div>
        </div>
      
          
        <div class="col-sm-4"></div>
        </div>
</div>
